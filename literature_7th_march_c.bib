@book{reinicke_2008,
	address = {Bielefeld},
	title = {Vorpommersche Boddenküste},
	abstract = {Rez.: Bodden sind Gewässer einer besonderen Art: Sie waren einst Ostseebuchten, sind aber mittlerweile überwiegend vom Meer abgeschnitten und enthalten schwach salziges Wasser. Die vorpommerschen Bodden, die der Autor (bekannt {z.B.} durch {"Insel} Rügen - Die Kreideküste", {BA} 8/07) hier vorstellt, bilden eine beeindruckende, vielgestaltige Küstenlandschaft und werden als Urlaubsort für Naturfreunde, Wanderer, Wassersportler oder Radfahrer immer beliebter. Beschrieben werden neben der Entstehungsgeschichte der Bodden die Beschaffenheit der Bodden selbst und ihrer Küstenlandschaft, das dortige Leben von Pflanzen, Tier und Mensch und dann die einzelnen Teile der Boddenküste, nämlich die Darss-Zingster-Boddenkette sowie Kubitzer, Greifswalder, westrügensche und nordrügensche Bodden. Der Text ist informativ und angenehm zu lesen. Wirklich gelungen und sehr beeindruckend sind die zahlreichen Fotografien, auf denen die Seele dieser einzigartigen Landschaft wirklich in bemerkenswerter Weise wiedergegeben wird. Deutlich aktueller als {"Boddenküste"} vom gleichen Autor ({ID} 29/96). (2) (Heidi Debschütz)},
